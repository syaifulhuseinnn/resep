{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
		"no-console": "off",
		"func-names": ["error", "never"],
		"no-use-before-define": ["error", { "functions": false }],
		"no-tabs": [
			"error",
			{ "allowIndentationTabs": true }
		],
		"quotes": [
			"error",
			"double"
		],
		"import/extensions": ["error", "always"],
		"no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
		"no-else-return":"off"
    }
}
